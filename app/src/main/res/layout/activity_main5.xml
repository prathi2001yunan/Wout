<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:backgroundTint="#F8040000"
    tools:context=".MainActivity5">

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImageView"
        android:layout_width="404dp"
        android:layout_height="217dp"
        android:layout_marginTop="49dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.182"
        tools:ignore="MissingConstraints">

    </pl.droidsonroids.gif.GifImageView>

    <FrameLayout
        android:id="@+id/fram31"
        android:layout_width="170dp"
        android:layout_height="148dp"
        android:layout_marginTop="131dp"
        android:background="@drawable/sample1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65"
        tools:ignore="ExtraText,MissingConstraints">

        <TextView
            android:id="@+id/tvn1"
            android:layout_width="122dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="10"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="40dp">

        </TextView>

        <ProgressBar
            android:id="@+id/progress11"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="159dp"
            android:layout_height="133dp"
            android:layout_gravity="center"
            android:background="@drawable/sample2"
            android:indeterminate="false"
            android:max="30"
            android:progress="300"
            android:progressDrawable="@drawable/sample3">

        </ProgressBar>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/fram331"
        android:layout_width="188dp"
        android:layout_height="178dp"
        android:layout_marginTop="151dp"
        android:background="@drawable/sample1"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65"
        tools:ignore="ExtraText,MissingConstraints">
        <androidx.appcompat.widget.Toolbar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        </androidx.appcompat.widget.Toolbar>

        <TextView
            android:id="@+id/tvn31"
            android:layout_width="122dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="10"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="40dp">

        </TextView>

        <ProgressBar
            android:id="@+id/progress311"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="159dp"
            android:layout_height="133dp"
            android:layout_gravity="center"
            android:background="@drawable/sample2"
            android:indeterminate="false"
            android:max="20"
            android:progress="200"
            android:progressDrawable="@drawable/sample3">

        </ProgressBar>
    </FrameLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="413dp"
        android:layout_height="40dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="59dp"
        android:fontFamily="@font/aclonica"
        android:text="SQUAT"
        android:textAlignment="center"
        android:textColor="#FBF9F9"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.496" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="118dp"
        android:layout_height="50dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="248dp"
        android:layout_marginBottom="19dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="EXERCISE"
        android:textColor="#F6EBEB"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/gifImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="201dp"
        android:text="1"
        android:textAlignment="viewStart"
        android:textColor="#FAF7F7"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/fram9"
        android:visibility="gone"
        android:layout_width="80dp"
        android:layout_height="82dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/efram2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fram31">
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:src="@drawable/pause1"
            >

        </ImageView>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/fram91"
        android:layout_width="80dp"
        android:layout_height="82dp"
        android:layout_marginStart="276dp"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="6dp"
        android:background="@drawable/efram2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fram31"
        app:layout_constraintVertical_bias="0.0" >
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:src="@drawable/next"
            >

        </ImageView>


    </FrameLayout>

    <FrameLayout
        android:id="@+id/fram911"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="6dp"
        android:background="@drawable/efram2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fram31"
        app:layout_constraintVertical_bias="0.036">

        <ImageView
            android:layout_width="53dp"
            android:layout_height="61dp"
            android:layout_gravity="center"
            android:src="@drawable/pause">

        </ImageView>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fram9111"
        android:layout_width="80dp"

        android:layout_height="80dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="6dp"
        android:background="@drawable/efram2"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fram31"
        app:layout_constraintVertical_bias="0.036">

        <ImageView
            android:layout_width="53dp"
            android:layout_height="61dp"
            android:layout_gravity="center"
            android:src="@drawable/resume"></ImageView>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fram8"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="69dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/simple"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:src="@drawable/skip1"
            tools:layout_editor_absoluteX="307dp"
            tools:layout_editor_absoluteY="3dp">

        </ImageView>

    </FrameLayout>



</androidx.constraintlayout.widget.ConstraintLayout>